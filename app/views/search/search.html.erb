<div class="container">
  <h1>Rakuten商品検索</h1>
  <div class='content'>
    <div id="search-box">
      <%= form_with url: search_path, method: :get, local: true do |f| %>
        <%= f.text_field :keyword, class: "form-control" %>
        <div class="input-group-append">
          <%= f.submit "検索", class: "btn btn-primary" %>
        </div>
      <% end %>
    </div>
    <h2>検索結果</h2>
    <div id='good_list'>
       <%= render 'layouts/item_list' %>
    </div>
  </div>
</div>
