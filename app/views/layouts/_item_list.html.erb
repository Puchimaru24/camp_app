<% if @items.present? %>
  <% @items.first(10).each do |item| %>
    <div class="items-index-item">
      <div class="search-img">
        <img src="<%= item["mediumImageUrls"][0] %>" alt="rakuten img">
      </div>
      <div class="search-name">
        <%= link_to item.name, "#{item.url}" %>
      </div>
      <div class="search-price">
        <%= number_with_delimiter(item.price) %>å††
      </div>
    </div>
  <% end %>
<% end %>